<div class="style_list_item client_item" id="contact_{CONTACT_ID}">
<div class="contact_hide_content_{CONTACT_ID}">
{EDIT_TOOLS}
</div>
<div id="contact_content_{CONTACT_ID}" class="contact_hide_content_{CONTACT_ID}">
{IMAGE_BLOCK}
<table cellpadding="0" cellspacing="0" class="style_list_item_tb">
	<tr style="{USER_NAME_DISPLAY}">
    	<td class="td_title">ФИО</td>
        <td class="td_value">{CONTACT_USER_NAME}</td>
        <td class="error"></td>
    </tr>
    <tr style="{CONTACT_NAME_DISPLAY}">
    	<td class="td_title">Название контакта</td>
        <td class="td_value">{CONTACT_NAME}</td>
        <td class="error"></td>
    </tr>  
    <tr style="{PHONE_DISPLAY}">
    	<td class="td_title">Телефон</td>
        <td class="td_value">{CONTACT_PHONE}</td>
        <td class="error"></td>
    </tr>
    <tr style="{JOB_DISPLAY}">
    	<td class="td_title">Должность</td>
        <td class="td_value">{CONTACT_JOB}</td>
        <td class="error"></td>
    </tr>
    <tr style="{DESC_DISPLAY}">
    	<td class="td_title td_vert_top">Описание</td>
        <td class="td_value">{CONTACT_DESC}</td>
        <td class="error"></td>
    </tr>  
</table><div class="clear"></div>
<div style="margin-top:5px"> 
<div class="client_added_by">Добавил: {USER_SURNAME} {USER_NAME} {USER_MIDDLENAME}</div>
</div>
 
</div>
<div id="contact_result_{CONTACT_ID}"></div>

</div>